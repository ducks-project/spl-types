{
    "name": "ducks-project/spl-types",
    "description": "This extension aims at helping people making PHP a stronger typed language and can be a good alternative to scalar type hinting. It provides different typehandling classes as such as integer, float, bool, enum and string",
    "version": "1.0.0",
    "type": "library",
    "keywords": ["Enum", "SplTypes", "SplEnum", "enumerator", "POO", "SplBool", "SplFloat", "SplInt", "SplString", "SplType"],
    "homepage": "https://github.com/ducks-project/spl-types/",
    "license": "MIT",
    "authors": [
        {
            "name": "Adrien Loyant",
            "email": "donald_duck@team-df.org",
            "homepage": "http://adrien.loyant.net",
            "role": "Owner"
        }
    ],
    "support": {
        "email": "adrien.loyant@gmail.com"
    },
    "require": {
        "php": ">=5.3.3"
    },
    "autoload": {
        "psr-4": {
            "Ducks\\Component\\": "src/Ducks/Component/"
        },
        "files": [
            "polyfill-spltypes.php"
        ]
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
